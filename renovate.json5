{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  schedule: "at 02:00am every day",
  prConcurrentLimit: 0,
  prHourlyLimit: 0,
  rangeStrategy: "bump",
  packageRules: [
    {
      matchFileNames: ["**/package.json"],
      matchPackageNames: ["node"],
      matchManagers: ["npm"],
      matchDepTypes: [ "engines" ],
      enabled: false
    }
  ]
}
