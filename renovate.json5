{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  prConcurrentLimit: 0,
  prHourlyLimit: 0,
  rangeStrategy: "bump",
  packageRules: [
    {
      matchFileNames: ["packages/*/package.json"],
      matchPackageNames: ["node", "@types/node"],
      enabled: false
    }
  ]
}
