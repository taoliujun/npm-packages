{
  "name": "webpack-plugin-noop-ts",
  "version": "1.1.9",
  "description": "webpack plugin for typescript that does nothing",
  "keywords": [
    "webpack",
    "plugin",
    "noop"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "taoliujun <taoliujun@qq.com>",
  "homepage": "https://github.com/taoliujun/npm-packages/tree/master/packages/webpack-plugin-noop-ts",
  "license": "MIT",
  "types": "lib/types/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "/*.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taoliujun/npm-packages.git"
  },
  "scripts": {
    "lint:ts": "tsc -p ./tsconfig.json --noEmit",
    "clean": "rm -rf lib/",
    "cjs": "tsc --module commonjs --outDir lib/cjs",
    "esm": "tsc --module es6 -d --declarationDir lib/types --outDir lib/esm",
    "build": "yarn run clean && yarn run cjs && yarn run esm"
  },
  "bugs": {
    "url": "https://github.com/taoliujun/npm-packages/issues"
  },
  "gitHead": "1e04cfaeb31b8c8c2319b2cfd484d926c3599ade"
}
